{
   "name": "website-components-auth",
   "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
   "sourceRoot": "libs/website/components/auth/src",
   "projectType": "library",
   "tags": [],
   "targets": {
      "lint": {
         "executor": "@nx/linter:eslint",
         "outputs": ["{options.outputFile}"],
         "options": {
            "lintFilePatterns": [
               "libs/website/components/auth/**/*.{ts,tsx,js,jsx}"
            ]
         }
      },
      "build": {
         "executor": "@nx/vite:build",
         "outputs": ["{options.outputPath}"],
         "defaultConfiguration": "production",
         "options": {
            "outputPath": "dist/libs/website/components/auth"
         },
         "configurations": {
            "development": {
               "mode": "development"
            },
            "production": {
               "mode": "production"
            }
         }
      },
      "test": {
         "executor": "@nx/jest:jest",
         "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
         "options": {
            "jestConfig": "libs/website/components/auth/jest.config.ts",
            "passWithNoTests": true
         },
         "configurations": {
            "ci": {
               "ci": true,
               "codeCoverage": true
            }
         }
      }
   }
}
